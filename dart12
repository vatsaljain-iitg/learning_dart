// Named Parameters - The usage of names of variables whiel calling them is compulsory
// void main()
// {
//   findvolume(2,height:10,breadth:3);
// }
// void findvolume(int length,{int breadth,int height}) =>
//   print("The volume of the cuboid is ${length*breadth*height}");



void main()
{
  findvolume(2,3);
  findvolume(2,3,height:20); // this will override the default parameter
}
void findvolume(int length,int breadth , {int height = 10}) // height is set to 10 by default
{
  print("The volume is ${length*breadth*height}");
}
